---
import { storyblokEditable } from '@storyblok/astro'
import StoryblokComponent from '@storyblok/astro/StoryblokComponent.astro'

const { blok } = Astro.props
---
<!-- data-aos="fade-up" data-aos-duration="200" data-aos-offset="400" data-aos-mirror="true" -->
<section {...storyblokEditable(blok)}  class="flex flex-col gap-4 py-8">
    { blok.items.map((subBlok: any) => <StoryblokComponent blok={subBlok} /> )}
</section>
