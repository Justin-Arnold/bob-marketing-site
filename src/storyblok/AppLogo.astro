---
import { storyblokEditable } from '@storyblok/astro'

const { blok } = Astro.props
---

<a {...storyblokEditable(blok)} class="h-12 py-2 flex items-center gap-2" href="/">
    <img src={blok.logo.filename} alt="Astro Logo" class="h-full" />
    <h1 class="text-base-100 text-2xl font-thin">{blok.name}</h1>
</a>